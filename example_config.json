	"commands":{
		"!toaster":{
			"response":"<%= nick %> the <%= title %> says, \"Ding!\"",
			"minMegabotLevel":1
		},
		"!test":{
			"gfx":"images/angryseal.gif",
			"pause":3,
			"minMegabotLevel":1,
			"sound":"sounds/syfy.mp3"
		},
		"!roomba":{
			"response":"<%= nick %> the <%= title %> says, \"Out of my way!\"",
			"minLevel":10
		}
	},
	"megabotLevels":{
		"Toaster":1,
		"Roomba":10,
		"Paranoid Android":25,
		"Protocol Droid":50,
		"Claptrap":100,
		"Bender":250,
		"Iron Giant":500,
		"Cylon":1000,
		"Terminator":2000,
		"Sentinal":4000,
		"Transformer":8000
	},
	"subscriptionLevels":{
		"Shiny":{"amount":2, "rate":60},
		"Clever":{"amount":5, "rate":50},
		"Killer":{"amount":10, "rate":40},
		"Freaking Amazing":{"amount":25, "rate":30},
		"Ultimate Supreme":{"amount":50, "rate":20},
		"Robot God":{"amount":250, "rate":5}
	},
	"messages":{
		"minutes":5,
		"welcome":"<%= name %> welcomes back <%= nick %>",
		"newuser":"<%= nick %>, welcome to the channel",
		"newfollow":"<%= nick %>, Thanks for the follow, Here's <%= gift %> nanobots for you to enjoy.",
		"levelup":"<%= nick %> leveled up! - <%= title %>",
		"rotation":[
			"Check me out every week night",
			"more games and raffles coming up....",
			"pro tip: type \"!level\" to see your megabot level"
		]
	},
	"import":{
		"modbot":"import/ModBot.sqlite",
		"linebot":"import/accounts"
	}

}