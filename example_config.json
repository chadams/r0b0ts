    "server": "irc.twitch.tv",
    "port": 6667,
    "nickname": "r0b0ts_bot",
    "oauth": "oauth",
    "debug": true,
    "twitchclient": 1
	},
	"database":{
		"filename":"datafile.db",
		"minToSave":5
